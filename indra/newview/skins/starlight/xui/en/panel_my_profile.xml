<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="532"
 label="Profile"
 layout="topleft"
 left="0"
 name="panel_profile"
 top="0"
 width="313">
    <string
     name="CaptionTextAcctInfo">
        [ACCTTYPE]
[PAYMENTINFO] [AGEVERIFICATION]
    </string>
    <string
     name="payment_update_link_url">
        http://www.secondlife.com/account/billing.php?lang=en
    </string>
    <string
     name="partner_edit_link_url">
        http://www.secondlife.com/account/partners.php?lang=en
    </string>
    <string
     name="my_account_link_url"
     value="http://secondlife.com/account" />
    <string
     name="no_partner_text"
     value="None" />
    <string
     name="no_group_text"
     value="None" />
    <string
	 name="RegisterDateFormat">
	 [REG_DATE] ([AGE])
	</string>
	    <string
      name="name_text_args">
      [NAME]
    </string>
    <string
      name="display_name_text_args">
      [DISPLAY_NAME]
    </string>
   <accordion
    left="0"
    top="0"
    single_expansion="true"
    fit_parent="true"
    follows="all"
    layout="topleft"
    name="profile_accordion"
    height="505"
    width="313">
     <accordion_tab
      expanded="true"
      layout="topleft"
      name="second_life_tab"
      title="Second Life"
      fit_panel="false">
        <scroll_container
         color="SL-Background_66"
         follows="all"
         layout="topleft"
         left="0"
         name="profile_sl_scroll"
         opaque="true"
         height="505"
         width="313"
         top="0">
          <panel
            layout="topleft"
            follows="left|top|right"
            height="505"
            name="scroll_content_sl_panel"
            top="0"
            left="0"
            width="313">
              <texture_picker
               allow_no_texture="true"
               default_image_name="None"
               enabled="false"
               fallback_image="Generic_Person_Large"
               follows="top|left"
               height="144"
               layout="topleft"
               left="0"
               name="2nd_life_pic"
               top="10"
               width="154" />
            <text
             follows="left|top"
             font.style="BOLD"
             height="10"
             layout="topleft"
             left_pad="10"
             name="title_member_text"
             text_color="White"
             top_delta="0"
             value="Resident Since:"
             width="120" />
            <text_editor
             allow_scroll="false"
             bg_visible="false"
             follows="left|top"
             h_pad="0"
             height="15"
             layout="topleft"
             name="register_date"
             read_only="true"
             translate="false"
             v_pad="0"
             value="05/31/2376"
             width="120"
             word_wrap="true" />
            <text
             follows="left|top"
             font.style="BOLD"
             height="15"
             layout="topleft"
             name="title_acc_status_text"
             text_color="White"
             top_pad="12"
             value="Account Status:"
             width="120"
             word_wrap="true" />
            <text_editor
             allow_scroll="false"
             bg_visible="false"
             follows="left|top"
             h_pad="0"
             height="42"
             layout="topleft"
             name="acc_status_text"
             read_only="true"
             translate="false"
             top_pad="-2"
             v_pad="0"
             width="120"
             word_wrap="true">
              Resident. No payment info on file.
              Linden.
            </text_editor>
            <text
             follows="left|top"
             font.style="BOLD"
             height="15"
             layout="topleft"
             name="title_partner_text"
             text_color="White"
             top_pad="0"
             value="Partner:"
             width="120" />
            <panel
             follows="left|top"
             height="30"
             layout="topleft"
             name="partner_data_panel"
             top_pad="-2"
             width="120">
              <text
               follows="left|top"
               height="10"
               initial_value="(retrieving)"
               layout="topleft"
               left="0"
               link="true"
               name="partner_text"
               top="0"
               use_ellipses="true" 
               width="300" />
            </panel>
            <text
             follows="left|top"
             font.style="BOLD"
             height="13"
             layout="topleft"
             left="4"
             name="About_sl_text"
             text_color="White"
             top_pad="-7"
             value="About:"
             width="40" />
              <expandable_text
              allow_scroll="true"
			  follows="top|left|right"
               height="146"
               layout="topleft"
               left="3"
               textbox.max_length="512"
               textbox.show_context_menu="true"
               name="sl_description_edit"
               top_pad="0"
			   width="290"
               translate="false"
               expanded_bg_visible="true"
               expanded_bg_color="SL-Background_Light">
                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Aenean viverra orci et justo sagittis aliquet.Nullamma lesuada mauris sit amet ipsum. adipiscing elit. Ae nean viverra orci et justo sagittis aliquet. Nullam malesuada mauris sit amet ipsum. adipiscing elit. Aenean viverra orci et justo sagittis aliquet. Nullam malesuada mauris sit amet ipsum.
              </expandable_text>
            <text
             follows="left|top"
             font.style="BOLD"
             height="13"
             layout="topleft"
             left="4"
             name="title_my_groups_text"
             text_color="White"
             top_pad="3"
             value="Groups:"
             width="290" />
			<group_list
             name="group_list"
			 allow_scroll="true"
             left_delta="2"
             top_delta="15"
             right="-20"
             height="89"
			 for_agent="true"
			 allow_select="true"
             follows="all"
             layout="topleft" />
			<text
             follows="left|top"
             font.style="BOLD"
             height="13"
             layout="topleft"
             left="4"
             name="Key"
             text_color="White"
             top_pad="8"
             value="Key:"
             width="40" />
			<text_editor
             name="user_key"
             label="Your Key:"
             left="5"
             top_pad="2"
             right="-86"
             height="20"
             h_pad="0"
             v_pad="4"
             bg_visible="true"
             enabled="false"
             allow_scroll="false"
             initial_value="(loading ...)"
             follows="top|left|right"
             layout="topleft" />
            <button
             name="copy_uri"
             left_pad="4"
             right="-4"
             height="20"
             label="Copy URI"
             enabled="false"
             follows="top|right" />			 
          </panel>        
        </scroll_container>
      </accordion_tab>
    <accordion_tab
     expanded="false"
     layout="topleft"
     name="real_world_tab"
     title="Real World"
     fit_panel="false">
        <scroll_container
         color="SL-Background_66"
         follows="all"
         layout="topleft"
         left="0"
         name="profile_rl_scroll"
         opaque="true"
         height="420"
         width="313"
         top="0">
           <panel
            layout="topleft"
            follows="left|top|right"
            height="420"
            name="profile_rl_scroll_panel"
            top="0"
            left="0"
            width="313">
              <texture_picker
               allow_no_texture="true"
               default_image_name="None"
               enabled="false"
               fallback_image="Generic_Person_Large"
               follows="top|left"
               height="144"
               layout="topleft"
               left="0"
               name="real_world_pic"
               top="10"
               width="154" />
            <text
             follows="left|top"
             font.style="BOLD"
             height="13"
             layout="topleft"
             left="4"
             name="About_rl_text"
             text_color="White"
             top_pad="-15"
             value="About:"
             width="290" />
              <expandable_text
			   allow_scroll="true"
               follows="top|left|right"
               height="220"
			   width="290"
               layout="topleft"
               left="5"
               top_pad="-3"
               textbox.max_length="512"
               textbox.show_context_menu="true"
               name="fl_description_edit"
               translate="false"
               expanded_bg_visible="true"
               expanded_bg_color="SL-Background_Light">
                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Aenean viverra orci et justo sagittis aliquet.Nullamma lesuada mauris sit amet ipsum. adipiscing elit. Ae nean viverra orci et justo sagittis aliquet. Nullam malesuada mauris sit amet ipsum. adipiscing elit. Aenean viverra orci et justo sagittis aliquet. Nullam malesuada mauris sit amet ipsum.
              </expandable_text>
            <text
             follows="left|top"
             font.style="BOLD"
             height="13"
             layout="topleft"
             left="4"
             name="homepage_text"
             text_color="White"
             top_pad="3"
             value="Homepage:"
             width="290" />
              <text
              follows="left|top"
              height="15"
              layout="topleft"
              left="4"
              name="homepage_edit"
              top_pad="2"
              translate="false"
              value="blank"
              width="290"
              word_wrap="false"
              use_ellipses="true" />
            </panel>
        </scroll_container>
      </accordion_tab>
    </accordion>
 	<panel
     follows="bottom|left|right"
     height="23"
     layout="topleft"
     left="0"
     top_pad="4"
     name="profile_me_buttons_panel"
     visible="false"
     width="315">
        <button
         follows="bottom"
         height="23"
         left="6"
	 	 top="1"
         label="Edit Profile"
         name="edit_profile_btn"
         tool_tip="Edit your personal information"
         width="90" />
 	</panel>
</panel>
